RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^js/cart.js$    index.php?page=cart.js [L,NC]

RewriteRule ^multisearch$    index.php?page=multisearch [L,NC]

RewriteRule ^advancedsearch$    index.php?page=advancedsearch [L,NC]

RewriteRule ^unigene-details/byId/(.*)?$  index.php?page=unigene-details-byid&feature_id=$1 [L,NC,QSA]
RewriteRule ^isoform-details/byId/(.*)?$  index.php?page=isoform-details-byid&feature_id=$1 [L,NC,QSA]

RewriteRule ^isoform-details/byOrganismAndUniquename/(.*)/(.*)?$  index.php?page=isoform-details&organism=$1&uniquename=$2 [L,NC,QSA]

RewriteRule ^graphs(?:/(.*))?$  index.php?page=graphs&query=$1 [L,NC,QSA]
